{
  "name": "rdepot-client",
  "version": "2.6.1",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-pattern .gitignore",
    "lint:check": "eslint -o reports/lint-report.xml --format junit --ignore-pattern .gitignore --max-warnings 0",
    "lint:staged": "eslint --fix $(git diff --diff-filter=d --cached --name-only)",
    "license:check": "node ./license_checker.js",
    "test:unit": "vitest --dir ./src/__tests__/unit",
    "test:unit:ui": "vitest --ui --dir ./src/__tests__/unit --sequence.concurrent",
    "test:unit:once": "vitest run --dir ./src/__tests__/unit",
    "test:unit:once:junit": "vitest --dir ./src/__tests__/unit --reporter=junit --outputFile.junit=reports/test-report-unit.xml --reporter=default run",
    "test:e2e": "npm run test:e2e:serial && npm run test:e2e:parallel",
    "test:e2e:setup": "sh teste2e.sh && npm run restore:firefox && npm run restore:chrome",
    "test:e2e:restore": "npm run restore:chrome && npm run restore:firefox",
    "test:e2e:cleanupCode": "sh src/__tests__/config/cleanupCode.sh",
    "test:e2e:serial": "PLAYWRIGHT_BLOB_OUTPUT_FILE=./reports/e2e/e2e-serial.zip npx playwright test --grep serial --workers 1 --reporter=blob",
    "test:e2e:parallel": "npm run test:e2e:restore && PLAYWRIGHT_BLOB_OUTPUT_FILE=./reports/e2e/e2e-parallel.zip npx playwright test --grep-invert serial --reporter=blob",
    "test:e2e:merge": "PLAYWRIGHT_JUNIT_OUTPUT_FILE=./reports/test-report-e2e.xml npx playwright merge-reports --reporter junit ./reports/e2e && rm -fr ./reports/e2e",
    "test:e2e:cleanup": "sh cleanup.sh",
    "test:coverage": "vitest run --coverage",
    "execute": "npm run license:check && npm run test:unit:once && npm run build",
    "prettier": "npx pretty-quick",
    "prettier:changes": "npx pretty-quick --staged --bail",
    "json:sort": "jsonsort ./src/locales/*",
    "prepare": "husky install",
    "restore": "./docker/restore.sh",
    "restore:firefox": "./docker/restore-firefox.sh",
    "restore:chrome": "./docker/restore-chrome.sh"
  },
  "dependencies": {
    "@casl/ability": "^6.4.0",
    "@casl/vue": "^2.2.1",
    "@mdi/font": "7.0.96",
    "@types/marked": "^5.0.1",
    "@vee-validate/zod": "^4.8.6",
    "@vueuse/components": "^11.2.0",
    "@vueuse/core": "^11.2.0",
    "apexcharts": "^4.1.0",
    "axios": "^1.7.7",
    "child_process": "^1.0.2",
    "javascript-time-ago": "^2.5.11",
    "json-sort-cli": "^4.0.4",
    "latest-version": "^9.0.0",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "marked": "^5.1.1",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "oidc-client-ts": "^2.2.4",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.1",
    "rfc6902": "^5.0.1",
    "roboto-fontface": "*",
    "rst-compiler": "^0.4.1",
    "vee-validate": "^4.11.7",
    "vite": "^5.4.8",
    "vue": "^3.2.38",
    "vue-dompurify-html": "^4.1.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0",
    "vue3-apexcharts": "^1.8.0",
    "vue3-toastify": "^0.1.14",
    "vuetify": "^3.7.18",
    "wait-for-expect": "^3.0.2",
    "webfontloader": "^1.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.16",
    "@playwright/test": "^1.47.2",
    "@rollup/rollup-linux-x64-gnu": "^4.22.5",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.11.9",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-c8": "0.28.3",
    "@vitest/ui": "^2.0.5",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-junit": "^8.40.0",
    "eslint-output": "^3.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.29.0",
    "flush-promises": "^1.0.2",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "msw": "^2.4.11",
    "pre-commit": "^1.2.2",
    "prettier": "3.3.3",
    "pretty-quick": "^4.0.0",
    "sass": "^1.57.1",
    "typescript": "^5.6.3",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  }
}
