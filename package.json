{
  "name": "rdepot-client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "lint:staged": "eslint --fix $(git diff --diff-filter=d --cached --name-only)",
    "license:check": "node ./license_checker.js",
    "test:unit": "vitest --environment jsdom",
    "test:unit:once": "vitest run",
    "test:unit:ui": "vitest --ui --environment jsdom",
    "test:coverage": "vitest  run --coverage",
    "execute": "npm run license:check && npm run test:unit:once && npm run build",
    "prettier": "npx pretty-quick",
    "prettier:changes": "npx pretty-quick --staged --bail",
    "prepare": "husky install"
  },
  "dependencies": {
    "@casl/ability": "^6.4.0",
    "@casl/vue": "^2.2.1",
    "@kyvg/vue3-notification": "^2.7.0",
    "@mdi/font": "7.0.96",
    "@vee-validate/zod": "^4.8.6",
    "@vueuse/components": "^9.13.0",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.2.2",
    "keycloak-js": "^20.0.2",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "mitt": "^3.0.1",
    "oidc-client": "^1.11.5",
    "pinia": "^2.0.23",
    "rfc6902": "^5.0.1",
    "roboto-fontface": "*",
    "vee-validate": "^4.8.6",
    "vue": "^3.2.38",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.16",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.11.9",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.27.3",
    "@vitest/ui": "^0.29.8",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.0",
    "flush-promises": "^1.0.2",
    "husky": "^8.0.3",
    "jsdom": "^21.0.0",
    "msw": "^1.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "2.8.8",
    "pretty-quick": "^3.1.3",
    "sass": "^1.57.1",
    "typescript": "^4.0.0",
    "vite": "^3.0.9",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vitest": "^0.27.1",
    "vue-tsc": "^1.0.9"
  }
}
